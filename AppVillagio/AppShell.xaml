<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:AppVillagio.Views"
       x:Class="AppVillagio.AppShell"
       Shell.FlyoutBehavior="Disabled">

	<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:WelcomePage}"
        Route="WelcomePage" />

	<ShellContent
        Title="Dashboard"
        ContentTemplate="{DataTemplate local:DashboardPage}"
        Route="DashboardPage" />

</Shell>